
generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ContactRequest {
  id         Int      @id @default(autoincrement())
  nombre     String
  email      String
  mensaje    String
  enviadoAt  DateTime @default(now())
}